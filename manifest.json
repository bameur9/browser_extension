{
  "manifest_version": 3,
  "name": "Blocker",
  "description": "Block content you don't want to see on the web.",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "options_ui": {
    "page": "opt.html"
  },
  "permissions": [
    "scripting"
  ]
}