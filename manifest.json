{
    "name": "Content Blocker",
    "description": "Blockiert bestimmte Begriffe auf Webseiten.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_icon": {
            "16": "images/icon-16.png",
            "32": "images/icon-32.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png"
        }
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "permissions": ["activeTab"],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content1.js"],
        "css": ["style.css"]
    }]

}